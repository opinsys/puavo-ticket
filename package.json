  "eslintConfig": {
    "parser": "babel-eslint",
    "rules": {
      "no-path-concat": false,
      "no-shadow": true,
      "quotes": false,
      "camelcase": false,
      "curly": [
        2,
        "multi-line"
      ],
      "consistent-return": false,
      "new-cap": false,
      "no-redeclare": [
        2
      ],
      "no-comma-dangle": false,
      "no-process-exit": false,
      "no-unused-vars": [
        2,
        {
          "vars": "all",
          "args": "none"
        }
      ],
      "eol-last": false,
      "no-underscore-dangle": false
    },
    "env": {
      "browser": true,
      "mocha": true,
      "node": true
    }
  },
  "jshintConfig": {
    "esnext": true,
    "undef": true,
    "unused": "vars",
    "node": true,
    "browser": true,
    "strict": true,
    "newcap": false,
    "lastsemic": true,
    "predef": [
      "-fetch",
      "-Comment",
      "-Notification",
      "-Promise"
    ],
    "globals": {
      "alert": true,
      "it": true,
      "describe": true,
      "before": true,
      "beforeEach": true,
      "afterEach": true,
      "after": true
    }
  }
